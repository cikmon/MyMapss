<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cik.mymapss.MainActivity">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        >

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fab"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:layout_gravity="top|left"
            android:onClick="onClick12"
            app:srcCompat="@android:drawable/ic_menu_sort_by_size"
             />

        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="-10dp"></fragment>

        <Button
        android:id="@+id/loconof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center"
        android:onClick="onClickonof"
        android:text="on/off loc"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="0dp" />
        <Button
            android:id="@+id/of"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top|right"
            android:onClick="onClickof"
            android:text="on/off loc"
            tools:layout_editor_absoluteX="257dp"
            tools:layout_editor_absoluteY="0dp" />

    </FrameLayout>




</android.support.constraint.ConstraintLayout>
